{
  "name": "request",
  "version": "0.0.1",
  "description": "requests for the browser and node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanfaucett/request.git"
  },
  "main": "src/index.js",
  "browser": "src/browser.js",
  "scripts": {
    "test": "npm run test-node",
    "test-node": "node ./test | ./node_modules/.bin/tap-spec",
    "test-node-raw": "node ./test",
    "test-browser-local": "zuul --local 9999 --ui tape -- ./test"
  },
  "keywords": [
    "request",
    "request.js"
  ],
  "author": "Nathan Faucett",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "^0.11.3",
    "grunt-jsbeautifier": "^0.2.10",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.0",
    "zuul": "^3.6.0"
  },
  "dependencies": {
    "capitalize_string": "git://github.com/nathanfaucett/capitalize_string.git",
    "event_emitter": "git://github.com/nathanfaucett/event_emitter.git",
    "extend": "git://github.com/nathanfaucett/extend.git",
    "filter": "git://github.com/nathanfaucett/filter.git",
    "object-for_each": "git://github.com/nathanfaucett/object-for_each.git",
    "array-for_each": "git://github.com/nathanfaucett/array-for_each.git",
    "is_function": "git://github.com/nathanfaucett/is_function.git",
    "is_string": "git://github.com/nathanfaucett/is_string.git",
    "map": "git://github.com/nathanfaucett/map.git",
    "methods": "git://github.com/nathanfaucett/methods.git",
    "promise_polyfill": "git://github.com/nathanfaucett/promise_polyfill.git",
    "trim": "git://github.com/nathanfaucett/trim.git",
    "urls": "git://github.com/nathanfaucett/urls.git",
    "xmlhttprequest_polyfill": "nathanfaucett/xmlhttprequest_polyfill.git"
  }
}
